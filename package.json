{
  "name": "react-babel-webpack-boilerplate",
  "version": "1.2.4",
  "description": "",
  "main": "app/main.jsx",
  "scripts": {
    "lint": "eslint 'app/**/*.@(js|jsx)'",
    "lint:msg": "validate-commit-msg",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --content-base app",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack -p --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "babel-runtime": "6.x",
    "body-style": "^0.3.0",
    "color": "^2.0.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "15.x",
    "react-dom": "15.x",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.2.0",
    "react-inlinesvg": "^0.8.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.5.4"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "6.x",
    "commitizen": "^2.5.0",
    "conventional-changelog-cli": "^1.3.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^0.11.2",
    "ghooks": "^2.0.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "raw-loader": "^0.5.1",
    "srcset-loader": "^2.2.2",
    "style-loader": "^0.18.2",
    "svg-loader": "^0.0.2",
    "svg-url-loader": "^2.1.1",
    "url-loader": "^0.5.9",
    "validate-commit-msg": "2.x",
    "webpack": "3.x",
    "webpack-dev-server": "2.x"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "Ruan Yifeng",
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "npm run lint:msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
